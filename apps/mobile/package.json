{
  "name": "mobile",
  "private": true,
  "main": "expo/AppEntry",
  "scripts": {
    "dev": "expo start",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "jest",
    "expo:doctor": "expo doctor",
    "expo:export:test": "expo export --platform web --force --output-dir tmp/expo-export",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "verify:api": "API_BASE_URL=$API_BASE_URL API_TOKEN=$API_TOKEN jest __tests__/api.verify.spec.js -i"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.26",
    "@tanstack/react-query": "^5.87.4",
    "axios": "^1.12.1",
    "expo": "^52.0.47",
    "expo-constants": "~17.0.8",
    "expo-linking": "~7.0.5",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~14.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.11",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^9.7.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.6",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.4.1",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": []
  },
  "version": "1.0.0"
}